{
  "name": "frontenddesktop-electronvue3",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "wrjin",
  "homepage": "https://jinjin123456.github.io/",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "start:dev": "electron-vite dev --mode development",
    "start:test": "electron-vite dev --mode test",
    "start:prod": "electron-vite dev --mode production",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "prepare": "husky install",
    "cz": "cz",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.3",
    "cesium": "^1.112.0",
    "echarts": "^5.4.3",
    "electron-updater": "^6.1.1",
    "element-plus": "^2.4.3",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "moment": "^2.30.1",
    "ol": "^8.2.0",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/node": "^18.17.5",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.8.0",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,json}": "eslint --fix"
  },
  "volta": {
    "node": "18.19.0"
  }
}
